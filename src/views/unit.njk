{% extends "src/views/default.njk" %}

{% block content %}
  <h1 class="text-center">{{ module.name }}</h1>
  <h2>{{ unitIndex }}. {{ unit.name }} </h2>
  <h3>Resources</h3>
  <ul>
    {% for resource in unit.resources %}
    {% set url = modules.buildResourceURL(module, unit, resource) %}

    <li><a href="{{ url }}">{{ resource.name }}</a></li>
    {% endfor %}
  </ul>

{% endblock %}
